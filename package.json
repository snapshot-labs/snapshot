{
  "name": "snapshot",
  "version": "0.1.4",
  "scripts": {
    "preinstall": "yarn run init-submodules",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint \"./src/*/**/*.{ts,vue,json}\" --fix",
    "postinstall": "patch-package && husky install",
    "init-submodules": "git submodule update --init",
